{
    "name": "grapecity-grapeleaf-common",
    "version": "1.0.0",
    "description": "Common library for GrapeLeaf React Project",
    "license": "ISC",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "cbp": "npm run clean && npm run build && npm publish",
        "dcbpr": "del /F /Q *.tgz && npm run clean && npm run build && npm pack && ren *.tgz grapecity-grapeleaf-common.tgz",
        "build": "tsc --outDir lib --declaration --declarationDir lib  && copyfiles -u 1 src/**/*.less src/**/*.json src/**/*.css src/**/fonts/* lib",
        "clean": "rimraf dist lib",
        "dev": "cross-env NODE_ENV=development CONFIG_FILE=dev webpack-dev-server --hot  --inline --colors --config webpack.config.js",
        "start": "npm run dev",
        "test": "npm run test:loc",
        "test:loc": "mocha --watch --require config/dom.env.js --compilers ts:ts-node/register,tsx:ts-node/register -r ts-node/register src/**/*.test.ts* --colors",
        "test:prod": "mocha --require config/dom.env.js -r ts-node/register src/**/*.test.ts* --colors --reporter tap > test-reporter.log",
        "test:jenkins-report": "mocha --require config/dom.env.js -r ts-node/register src/**/*.test.ts* --colors -R mocha-jenkins-reporter -O junit_report_path=report.xml,junit_report_stack=1"
    },
    "keywords": [
        "grapecity",
        "grapeleaf",
        "common"
    ],
    "files": [
        "dist",
        "lib"
    ],
    "author": "GrapeCity",
    "dependencies": {
        "antd": "^2.11.0",
        "react": "15.4.1",
        "react-intl": "2.3.0",
        "react-redux": "^5.0.3",
        "react-router": "^3.0.0",
        "redux-actions": "^2.0.1",
        "react-dom": "^15.4.1",
        "redux": "^3.7.2",
        "superagent": "3.3.1",
        "inversify": "^4.3.0",
        "inversify-inject-decorators": "^3.0.1",
        "reflect-metadata": "^0.1.10"
    },
    "devDependencies": {
        "@types/chai": "^4.0.4",
        "@types/mocha": "^2.2.43",
        "@types/node": "7.0.10",
        "@types/react": "15.0.11",
        "@types/react-dom": "0.14.23",
        "@types/react-intl": "2.2.8",
        "@types/react-router": "^3.0.0",
        "@types/redux-actions": "^1.2.3",
        "@types/superagent": "2.0.36",
        "awesome-typescript-loader": "^3.0.7",
        "chai": "^4.1.2",
        "cross-env": "3.1.3",
        "css-loader": "0.26.1",
        "enzyme": "^2.9.1",
        "extract-text-webpack-plugin": "^2.1.0",
        "html-webpack-plugin": "2.24.1",
        "jsdom": "^11.2.0",
        "json-loader": "0.5.4",
        "less": "2.7.1",
        "less-loader": "2.2.3",
        "mocha": "^3.5.3",
        "mocha-jenkins-reporter": "^0.3.9",
        "mockjs": "^1.0.1-beta3",
        "moment": "^2.18.1",
        "moq.ts": "^2.5.1",
        "react-addons-test-utils": "^15.6.0",
        "react-test-renderer": "^15.6.1",
        "script-ext-html-webpack-plugin": "^1.8.5",
        "style-loader": "0.13.1",
        "ts-loader": "1.3.3",
        "ts-node": "^3.3.0",
        "typescript": "^2.3.2",
        "url-loader": "0.5.7",
        "webpack": "2.3.1",
        "webpack-dev-server": "^2.4.1"
    }
}
